{
	"name": "redaxmedia/redaxscript-download-sync",
	"description": "Yet another cronjob for Redaxscript's website",
	"version": "3.1.0",
	"homepage": "http://redaxscript.com",
	"license": "GPL-3.0",
	"require":
	{
		"php": ">5.4"
	},
	"require-dev":
	{
		"redaxmedia/redaxscript": "dev-master"
	},
	"scripts":
	{
		"post-install-cmd":
		[
			"cd vendor/redaxmedia/redaxscript && composer install"
		],
		"post-update-cmd":
		[
			"@post-install-cmd"
		]
	}
}
